{
    "name": "klkvsk/beeline-ord-api-client",
    "description": "API client for Beeline ORD system",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BeelineOrd\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Misha Kulakovsky",
            "email": "m@klkvsk.ru"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "7.4 || >=8",
        "psr/http-message": "^1.0",
        "psr/http-client": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/discovery": "^1.0",
        "php-http/httplug": "^2.0",
        "php-http/message-factory": "^1.0",
        "psr/log": "^3.0",
        "php-http/multipart-stream-builder": "^1.2"
    },
    "require-dev": {
        "klkvsk/dto-generator": "^0.5.1",
        "php-http/mock-client": "^1.5",
        "php-http/curl-client": "^2.2",
        "php-http/logger-plugin": "^1.3",
        "nyholm/psr7": "^1.5",
        "phpunit/phpunit": "^10.0",
        "vimeo/psalm": "^5.7"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
